\begin_layout Plain Layout
Ecuaciones de Maxwell 
\end_layout

\end_inset


\end_layout

\begin_layout Frame
Para los potenciales en un gauge arbitrario:
\end_layout

\begin_layout Frame
Ecuaciones Homogéneas:
\end_layout

\begin_layout Frame
\begin_inset Formula 
\begin{equation}
\mathbf{B}=\nabla\times\mathbf{A};\:\:\mathbf{E}=-\nabla\varphi-\frac{\partial\mathbf{A}}{\partial t}.
\end{equation}

\end_inset


\end_layout

\begin_layout Frame
Ecuaciones No Homogéneas:
\begin_inset Formula 
\begin{equation}
\nabla^{2}\varphi+\frac{\partial}{\partial t}\left(\nabla\cdot\mathbf{A}\right)=\frac{\rho}{\varepsilon_{0}};\:\:\Square\mathbf{A}+\nabla\left(\nabla\cdot\mathbf{A}+\frac{1}{c^{2}}\frac{\partial\varphi}{\partial t}\right)=\mu_{0}\mathbf{J}.
\end{equation}

\end_inset


\end_layout

\begin_layout Separator

\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Densidad de Corriente 
\begin_inset Formula $\mathbf{J}$
\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Frame
Conservación de la carga:
\end_layout

\begin_layout Frame
\begin_inset Formula 
\begin{equation}
\nabla\cdot\mathbf{J}=-\frac{\partial\rho}{\partial t}.
\end{equation}

\end_inset


\end_layout

\begin_layout Frame
Ley de Ohm:
\end_layout

\begin_layout Frame
\begin_inset Formula 
\begin{equation}
\mathbf{J}=\sigma\mathbf{E},
\end{equation}

\end_inset

siendo 
\begin_inset Formula $\sigma$
\end_inset

 la conductividad.
 
\end_layout

